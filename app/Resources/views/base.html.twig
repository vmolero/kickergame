<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}Kicker Game
        {% endblock %}</title>
    {% stylesheets
    'assets/vendor/bootstrap/dist/css/bootstrap.css'
    'assets/vendor/jquery-ui/themes/smoothness/jquery-ui.css'
    '@AppBundle/Resources/public/css/dashboard.css'
    filter='cssrewrite' %}
    <link rel="stylesheet" href="{{ asset_url }}"/>

    {% endstylesheets %}
    {% block stylesheets %}{% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
</head>
<body>
<nav class="navbar navbar-inverse navbar-fixed-top">
    <div class="container-fluid">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar"
                    aria-expanded="false" aria-controls="navbar">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="#">Kicker Game Tool
            </a>
        </div>
    </div>
</nav>

<div class="container-fluid">
    <div class="row">

        <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
            <h1 class="page-header">Trivago Trial</h1>
            <h2 class="sub-header">{% block header2 %}{% endblock %}</h2>
            <iv class="table-responsive">
                {% block body %}{% endblock %}
        </div>
    </div>
</div>
</div>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
<script>window.jQuery || document.write('<script src="assets/vendor/jquery/dist/jquery.js"><\/script>')</script>
{# 'assets/vendor/jquery-ui/jquery-ui.js'
 '@AppBundle/Resources/public/js/scripts.js'

#}
{% javascripts
'assets/vendor/bootstrap/dist/js/bootstrap.min.js'
'assets/vendor/jqGrid/js/i18n/grid.locale-en.js'
'bundles/thracedatagrid/js/init-datagrid.js'
'assets/vendor/jqGrid/js/jquery.jqGrid.js' %}
<script src="{{ asset_url }}"></script>
{% endjavascripts %}
{% block javascripts %}{% endblock %}
</body>
</html>